<%= form_for(@destination) do |f| %>
  <%= f.label :name %><br>
  <%= f.text_area :name %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :image_url %><br>
  <%= f.text_area :image %><br>
  <%= f.hidden_field :tag_id, :value => params[:tag] %>
  <%= f.submit "Create" %>
<% end %>
